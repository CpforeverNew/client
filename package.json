{
  "name": "cpforever",
  "version": "1.9.0-beta",
  "description": "A rewrite of the Club Penguin client in HTML5",
  "scripts": {
    "devclient": "webpack serve --host=0.0.0.0",
    "editor": "phasereditor2d-launcher -project . -max-number-files 100000 -public",
    "build": "webpack --mode production --env obfuscate=true",
    "build:stage": "webpack --mode production --env obfuscate=false staging=true",
    "build-crumbs": "node ./utils/build-crumbs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cpforever/client.git"
  },
  "author": "sahmyool, DirkD, Hexeption",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cpforever/client/issues"
  },
  "homepage": "https://cpforever.net",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "core-js": "^3.16.4",
    "javascript-obfuscator": "^4.0.0",
    "phasereditor2d-launcher": "^3.35.0",
    "socket.io-client": "^4.4.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-obfuscator": "^3.5.1",
    "html-webpack-plugin": "^5.5.0"
  },
  "dependencies": {
    "@sentry/browser": "^7.11.1",
    "@sentry/tracing": "^7.11.1"
  }
